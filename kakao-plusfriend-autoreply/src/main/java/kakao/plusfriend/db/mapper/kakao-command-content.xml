<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kakaoCommandReply">
	<select id="selectRegularContent" parameterType="java.util.Map" resultType="java.util.Map">
		select 
			method_name
		from
			tbl_kakao_commands
		where
			command_content = #{command}
	</select>
</mapper>